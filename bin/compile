#!/usr/bin/env ruby

# sync output
$stdout.sync = true

$:.unshift File.expand_path("../../lib", __FILE__)
require "language_pack"
  env_data = ENV.map{ |k,v| "#{k}:#{v}"}.join(' ')
  puts "TESTING compile #{env_data} args #{ARGV[0]} : #{ARGV[1]}"

if pack = LanguagePack.detect(ARGV[0] + '/FPAdmin', ARGV[1])
  pack.log("compile") do
    puts "IN Logging block"
    pack.compile
  end
end
